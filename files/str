(fun (str/eq a b)
  (while (and (u8 a) (== (u8 a) (u8 b)))
   (set a (+ a 1))
   (set b (+ b 1)))
  (== (u8 a) (u8 b)))

(fun (str/skip-whitespace ptr)
  (while (or (== (u8 ptr) 32) (== (u8 ptr) 10))
   (set ptr (+ ptr 1)))
  ptr)

(fun (str/skip-non-whitespace ptr)
  (while (and (u8 ptr) (!= (u8 ptr) 32) (!= (u8 ptr) 10))
   (set ptr (+ ptr 1)))
  ptr)
