(code globals-export-comp
  (push x1)
  (set x1 comp-export-table)
  (jmp .test)
.loop
  (set l0 (u64 x1 8))
  (call globals-lookup)
  (set l1 (u64 x1))
  (set (glb-type l0) def-type-func)
  (set (glb-val l0) l1)
  (add x1 16)
.test
  (cmp x1 comp-export-table-end)
  (jb .loop)
  (pop x1)
