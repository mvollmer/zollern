(code comp-expr
  ;; l0 - codeptr
  ;; l1 - exp
  (call comp-expand)
  (assert-type l1 exp-type-number .not-number)
  (set l2 (exp-num-val l1))
  (code-emit l0 (set l0 0xFFFFFFFFFFFFFFFF) l2)
  (ret)
.not-number
  (assert-type l1 exp-type-symbol .not-symbol)
  (push l0)
  (set l0 l1)
  (call lookup)
  (cmp l0 def-type-local)
  (jne .not-local)
  (neg l1)
  (pop l0)
  (code-emit l0 (set l0 (u64 x1 0xFFFF)) l1)
  (ret)
.not-local
  (pop l0)
  (code-emit l0 (set l0 0x1234567812345678) l1)
  (ret)
.not-symbol
  (assert-type l1 exp-type-string .not-string)
  (jmp comp-string-literal)
.not-string
  (parse-exp l1 l2)
  (assert-type l2 exp-type-number .call)
  (set l2 (exp-num-val l2))
  (cmp l2 comp-expr-table-size)
  (jae syntax)
  (cmp (cc-level x5) 0)
  (ja .in-function)
  (cmp l2 comp-first-local-only)
  (jae syntax)
.in-function
  (set l3 comp-expr-table)
  (set l3 (u64 l3 l2))
  (code-check l0)
  (jmp l3)
.call
  (cmp (cc-level x5) 0)
  (je syntax)
  (push x0)
  (set x0 0)
  (push l2)
  (cmp l1 0)
  (je .done)
  (call comp-exprs)
  (set x0 l1)
  (code-emit l0 (push l0))
.done
  (pop l1)
  (call comp-expr)
  (code-emit l0 (call l0))
  (mul x0 8)
  (code-emit l0 (add sp 0x1234) x0)
  (pop x0)
  (ret))

;; Main

(data
