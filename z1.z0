  (push x1)
  (set x0 l1)
  (parse-exp x0 l1)
  (call comp-expr)
  (code-emit l0 (cmp l0 0))
  (code-emit l0 (je 0x1234))
  (set x1 l0)
  (parse-exp x0 l1)
  (call comp-expr)
  (code-emit l0 (jmp 0x1234))
  (set l1 l0)
  (sub l1 x1)
  (set (u32 x1 -4) l1)
  (set x1 l0)
  (parse-exp x0 l1)
  (call comp-expr)
  (set l1 l0)
  (sub l1 x1)
  (set (u32 x1 -4) l1)
  (pop x1)
  (pop x0)
  (ret))

