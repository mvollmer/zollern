(code comp-expr-table
  (8 comp-begin)  ; 0
  (8 comp-func)   ; 1
  (8 comp-macro)  ; 2
  (8 comp-label)  ; 3
  (8 comp-goto)   ; 4
  (8 comp-if)     ; 5
  (8 comp-unop)   ; 6
  (8 comp-binop)  ; 7
  (8 comp-polop)  ; 8
  (8 comp-var)    ; 9
  (8 comp-set)    ; 10
  (8 comp-loc)    ; 11
  (8 comp-memref) ; 12
  (8 comp-memset) ; 13
  comp-expr-table-end)

(def comp-first-local-only 3)

(def comp-expr-table-size (>> (- comp-expr-table-end comp-expr-table) 3))

(code comp-string-literal
  (add l1 (sizeof exp-string))
  (code-emit l0 (jmp 0x1234))
  (set l2 l0)
.loop
  (set l3 (u8 l1))
  (set (u8 l0) l3)
  (add l0 1)
  (add l1 1)
  (cmp l3 0)
  (jne .loop)
  (set l1 l0)
  (sub l1 l2)
  (set (u32 l2 -4) l1)
  (code-emit l0 (set l0 0x12345678) l2)
